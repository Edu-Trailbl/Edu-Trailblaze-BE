{
  "GlobalConfiguration": {
    "BaseUrl": "https://localhost:7165"
  },
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/Cart/get-cart-by-userId",
      "DownstreamQueryStringParameters": {
        "userId": "{username}"
      },
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "edu-cart-service.azurewebsites.net",
          "Port": 443
        }
      ],
      "UpstreamPathTemplate": "/api/Cart/{username}",
      "UpstreamHttpMethod": [ "GET" ]
    }
  ]
}
